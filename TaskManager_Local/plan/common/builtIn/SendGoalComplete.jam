
PLAN PERFORM isro:SendGoalComplete($goalName){
	ID: "http://www.arbi.com//SendGoalComplete"
	PRECONDITION:
		FACT Communication($communicationInstance);
		FACT isro:ReasonerAddress($reasoner);
	BODY:
		$data = "(GoalCompleted \"" + $goalName + "\")";
		$communicationInstance.inform($reasoner, $data);
	 
}