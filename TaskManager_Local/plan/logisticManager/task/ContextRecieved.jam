PREFIXES:
		PREFIX isro: <http://www.arbi.com//BuiltInPlan#>;

PLAN CONCLUDE context($context){
	ID: "http://www.arbi.com//BuiltInPlan#contextDefaultForLogisticManager"
	PRECONDITION:
		FACT GLUtility($glUtility);
	BODY:
		
		System.out.println("no update : " + $context);
		$glUtility.assertGL($context);
		RETRACT context($context);
		
	UTILITY : 10;
}

PLAN CONCLUDE context($context){
	ID: "http://www.arbi.com//BuiltInPlan#contextPersonCall"
	PRECONDITION:
		FACT Communication($communication);
		FACT GLUtility($glUtility);
		$glUtility.retrieveGLName($context) == "personCall";
	BODY:
		System.out.println("PersonCall Recieved!!!!");
		System.out.println($context);
		$callID = $glUtility.retrieveGLExpression($context, 0);
		$location = $glUtility.retrieveGLExpression($context, 1);
		$taskType = $glUtility.retrieveGLExpression($context, 2);
		$data = "(context (PersonCall \"" + $callID + "\" \"" + $location + "\" \"" + $taskType + "\"))";
		System.out.println($data);
		$communication.assertToLTM($data);
		RETRACT context($context);
	UTILITY : 100;
}

