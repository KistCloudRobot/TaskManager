PLAN PERFORM TaskStarted($goalID) {
	ID: "http://www.arbi.com//TaskCreatedPotenit"
	PRECONDITION :
		FACT isro:SemanticMapManagerAddress($address);
		FACT isro:robot($robot);
		FACT robotLocation($location);
		$location == "potenit";
	BODY :
		PERFORM isro:startLog("PERFORM", "TaskStarted");
		
		$context = "(TaskStarted \"real\" \"" + $goalID + "\" \"" + $robot+ "\")";
		PERFORM notify("logistic", $address, $context);
		
		PERFORM isro:endLog("PERFORM", "TaskStarted");
	UTILITY : 10;
}

