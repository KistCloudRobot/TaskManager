
PLAN PERFORM ReportActionResult($address, $actionID) {
	ID: "http://www.arbi.com//ReportActionResultToRobot"
	PRECONDITION : 
	BODY:
		PERFORM isro:startLog("PERFORM", "ReportActionResult");
		
		$data = "(ActionResult \"" + $actionID + "\" \"success\")";
		PERFORM inform("logistic", $address, $data);
				
		PERFORM isro:endLog("PERFORM", "ReportActionResult");
	UTILITY : 10;
}


