PREFIXES:
		PREFIX isro: <http://www.arbi.com//BuiltInPlan#>;
		
PLAN CONCLUDE PostCargoUnstoringPreparedReportedTo($worker,$goalID ,$goal) {
	ID: "http://www.arbi.com//BuiltInPlan#PostCargoUnstoringPreparedReportedTo"
	PRECONDITION:
	BODY:
		RETRACT PostCargoUnstoringPreparedReportedTo($worker,$goalID , $goal);
		POST PERFORM CargoUnstoringPreparedReportedTo($worker,$goalID , $goal);
		
	UTILITY: 10000; 
}

PLAN PERFORM CargoUnstoringPreparedReportedTo($manager, $goalID, $goal){
	ID: "http://www.arbi.com//CargoUnstoringPreparedReportedTo"
	PRECONDITION:
		FACT Communication($communicationInstance);
		FACT ExtraUtility($helper);
		FACT isro:ReasonerAddress($reasoner);
		
	BODY:
		PERFORM isro:startLog("ACHIEVE", "CargoUnstoringPreparedReportedTo");
		
		ASSERT CargoUnstoringPreparedReportedFrom($manager, $goalID, $goal);
		perform isro:SendGoalComplete("CargoUnstoringPreparedReportedTo",$manager,$goalID,$goal);
		
		PERFORM isro:endLog("ACHIEVE", "CargoUnstoringPreparedReportedTo");
	UTILITY: 10000; 
}


PLAN CONCLUDE CargoUnstoringPreparedReportedFrom($worker, $goalID, $goal){
	ID: "http://www.arbi.com//CargoUnstoringPreparedReportedFrom"
	PRECONDITION:
		FACT Communication($communicationInstance);
		FACT isro:ReasonerAddress($reasoner);
	BODY:
		PERFORM isro:startLog("CONCLUDE", "CargoUnstoringPreparedReportedFrom");
		
		System.out.println("Reported Goal : " + $goal);
		perform isro:SendGoalComplete("CargoUnstoringPreparedReportedFrom",$worker,$goalID,$goal);
		RETRACT CargoUnstoringPreparedReportedFrom($worker, $goalID, $goal);
		PERFORM isro:endLog("CONCLUDE", "CargoUnstoringPreparedReportedFrom");
	UTILITY: 10000; 
}
